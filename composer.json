{
    "name": "formflowpro/formflow-pro-enterprise",
    "description": "Enterprise WordPress plugin for advanced form processing with Autentique integration",
    "type": "wordpress-plugin",
    "license": "GPL-2.0-or-later",
    "authors": [
        {
            "name": "FormFlow Pro Team",
            "email": "dev@formflowpro.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "ext-json": "*",
        "ext-mbstring": "*",
        "ext-pdo": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "mockery/mockery": "^1.5",
        "squizlabs/php_codesniffer": "^3.7",
        "phpstan/phpstan": "^1.10",
        "php-stubs/wordpress-stubs": "^6.4",
        "php-stubs/woocommerce-stubs": "^8.0",
        "szepeviktor/phpstan-wordpress": "^1.3"
    },
    "autoload": {
        "psr-4": {
            "FormFlowPro\\": "includes/"
        },
        "classmap": [
            "includes/queue/",
            "includes/email/",
            "includes/cache/",
            "includes/logs/",
            "includes/pdf/",
            "includes/autentique/",
            "includes/ajax/",
            "includes/admin/",
            "includes/analytics/",
            "includes/shortcodes/",
            "includes/integrations/",
            "includes/FormBuilder/",
            "includes/Notifications/",
            "includes/PWA/",
            "includes/Payments/",
            "includes/Security/",
            "includes/AI/",
            "includes/MultiSite/",
            "includes/Database/",
            "includes/API/",
            "includes/Traits/",
            "includes/Core/",
            "includes/class-loader.php",
            "includes/class-i18n.php",
            "includes/class-activator.php",
            "includes/class-deactivator.php",
            "includes/class-services.php",
            "includes/class-cron-schedules.php",
            "includes/class-archive-manager.php",
            "includes/class-formflow-plugin.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "FormFlowPro\\Tests\\": "tests/"
        },
        "classmap": [
            "tests/unit/",
            "tests/integration/"
        ]
    },
    "scripts": {
        "test": "phpunit",
        "test:coverage": "phpunit --coverage-html coverage",
        "phpcs": "phpcs --standard=PSR12 includes/",
        "phpstan": "phpstan analyse includes/ --level=5",
        "format": "phpcbf --standard=PSR12 includes/"
    },
    "config": {
        "optimize-autoloader": true,
        "sort-packages": true,
        "allow-plugins": {
            "composer/installers": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
